udioRecorder from './components/media/AudioRecorder';\n\n// === PÃGINAS - IMPORTS CORRETOS ===\nimport LoginPage from './pages/LoginPage';\nimport DashboardPage from './pages/DashboardPage';\nimport ConnectionPage from './pages/ConnectionPage';\n\n// === ESTILOS ===\nimport './App.css';\nimport './index.css';\n\n// === CONFIGURAÃ‡Ã•ES ===\nconst API_URL = 'http://localhost:3001';\nconst SOCKET_URL = 'http://localhost:3001';\n\n// ====================================\n// SISTEMA DE MENSAGENS GLOBAL MELHORADO\n// ====================================\nconst MessageSystem = ({ children }) => {\n  const [socket, setSocket] = useState(null);\n  const [messages, setMessages] = useState({});\n  const [conversations, setConversations] = useState([]);\n  const [connectionStatus, setConnectionStatus] = useState('disconnected');\n  \n  // === NOVOS ESTADOS PARA FUNCIONALIDADES AVANÃ‡ADAS ===\n  const [favoriteChats, setFavoriteChats] = useState(new Set());\n  const [unreadChats, setUnreadChats] = useState(new Set());\n  const [chatNames, setChatNames] = useState({}); // Para nomes customizados\n  const [bitrixIds, setBitrixIds] = useState({}); // IDs do Bitrix por conversa\n  \n  const socketRef = useRef(null);\n\n  // Inicializar Socket.IO (mantido igual)\n  useEffect(() => {\n    console.log('ðŸ”Œ Inicializando Socket.IO...');\n    \n    const newSocket = io(SOCKET_URL, {\n      transports: ['polling'],\n      reconnectionAttempts: 10,\n      reconnectionDelay: 1000,\n      timeout: 20000,\n    });\n\n    if (typeof window !== 'undefined') {\n      window.socket = newSocket;\n    }\n\n    // Event Listeners bÃ¡sicos (mantidos)\n    newSocket.on('connect', () => {\n      console.log('âœ… Socket conectado:', newSocket.id);\n      setConnectionStatus('connected');\n      setSocket(newSocket);\n      socketRef.current = newSocket;\n    });\n\n    newSocket.on('disconnect', () => {\n      console.log('âŒ Socket desconectado');\n      setConnectionStatus('disconnected');\n    });\n\n    newSocket.on('reconnect', () => {\n      console.log('ðŸ”„ Socket reconectado');\n      setConnectionStatus('connected');\n    });\n\n    // EVENTOS WHATSAPP (mantidos + melhorados)\n    newSocket.on('whatsapp:message_received', (messageData) => {\n      console.log('ðŸ“¨ Mensagem recebida:', messageData);\n      \n      const conversationId = messageData.from;\n      setMessages(prev => ({\n        ...prev,\n        [conversationId]: [...(prev[conversationId] || []), {\n          id: messageData.id || Date.now(),\n          body: messageData.body || '',\n          from: messageData.from,\n          fromMe: false,\n          timestamp: messageData.timestamp || Date.now(),\n          type: 'received',\n          media: messageData.media,\n          hasMedia: messageData.hasMedia\n        }]\n      }));\n\n      // Marcar como nÃ£o lida automaticamente quando receber mensagem\n      setUnreadChats(prev => new Set([...prev, conversationId]));\n\n      setConversations(prev => {\n        const existing = prev.find(conv => conv.id === conversationId);\n        if (existing) {\n          return prev.map(conv => \n            conv.id === conversationId \n              ? { ...conv, lastMessage: messageData.body, timestamp: messageData.timestamp }\n              : conv\n          );\n        } else {\n          return [...prev, {\n            id: conversationId,\n            name: messageData.notifyName || conversationId.split('@')[0],\n            lastMessage: messageData.body,\n            timestamp: messageData.timestamp,\n            unreadCount: 1\n          }];\n        }\n      });\n\n      if (Notification.permission === 'granted') {\n        new Notification('Nova mensagem WhatsApp', {\n          body: messageData.body,\n          icon: '/favicon.ico'\n        });\n      }\n    });\n\n    newSocket.on('whatsapp:message_sent', (messageData) => {\n      console.log('ðŸ“¤ Mensagem enviada:', messageData);\n      \n      const conversationId = messageData.to;\n      setMessages(prev => ({\n        ...prev,\n        [conversationId]: [...(prev[conversationId] || []), {\n          id: messageData.id || Date.now(),\n          body: messageData.message || messageData.body || '',\n          to: messageData.to,\n          fromMe: true,\n          timestamp: messageData.timestamp || Date.now(),\n          type: 'sent',\n          status: 'sent'\n        }]\n      }));\n    });\n\n    newSocket.on('whatsapp:ready', (data) => {\n      console.log('ðŸš€ WhatsApp pronto:', data);\n      setConnectionStatus('ready');\n    });\n\n    newSocket.on('whatsapp:disconnected', () => {\n      console.log('âš ï¸ WhatsApp desconectado');\n      setConnectionStatus('disconnected');\n    });\n\n    newSocket.on('whatsapp:chats', (chats) => {\n      console.log('ðŸ’¬ Lista de chats recebida:', chats);\n      if (Array.isArray(chats)) {\n        setConversations(chats.map(chat => ({\n          id: chat.id._serialized,\n          name: chat.name,\n          lastMessage: chat.lastMessage?.body || '',\n          timestamp: chat.timestamp,\n          unreadCount: chat.unreadCount || 0,\n          isGroup: chat.isGroup\n        })));\n      }\n    });\n\n    return () => {\n      console.log('ðŸ§¹ Limpando Socket.IO...');\n      newSocket.disconnect();\n    };\n  }, []);\n\n  // Solicitar permissÃ£o de notificaÃ§Ã£o\n  useEffect(() => {\n    if (Notification.permission === 'default') {\n      Notification.requestPermission();\n    }\n  }, []);\n\n  // === FUNÃ‡Ã•ES AVANÃ‡ADAS ADICIONADAS ===\n  \n  // Favoritar/desfavoritar conversa\n  const toggleFavorite = (conversationId) => {\n    setFavoriteChats(prev => {\n      const newSet = new Set(prev);\n      if (newSet.has(conversationId)) {\n        newSet.delete(conversationId);\n        console.log('ðŸ’” Removido dos favoritos:', conversationId);\n      } else {\n        newSet.add(conversationId);\n        console.log('â­ Adicionado aos favoritos:', conversationId);\n      }\n      return newSet;\n    });\n  };\n\n  // Marcar/desmarcar como nÃ£o lida\n  const toggleUnread = (conversationId) => {\n    setUnreadChats(prev => {\n      const newSet = new Set(prev);\n      if (newSet.has(conversationId)) {\n        newSet.delete(conversationId);\n        console.log('âœ… Marcado como lido:', conversationId);\n      } else {\n        newSet.add(conversationId);\n        console.log('ðŸ“© Marcado como nÃ£o lido:', conversationId);\n      }\n      return newSet;\n    });\n  };\n\n  // Editar nome da conversa\n  const updateChatName = (conversationId, newName) => {\n    setChatNames(prev => ({\n      ...prev,\n      [conversationId]: newName\n    }));\n    \n    // Atualizar tambÃ©m na lista de conversas\n    setConversations(prev => \n      prev.map(conv => \n        conv.id === conversationId \n          ? { ...conv, name: newName }\n          : conv\n      )\n    );\n    \n    console.log('âœï¸ Nome alterado:', conversationId, '->', newName);\n  };\n\n  // Salvar ID do Bitrix\n  const setBitrixId = (conversationId, bitrixId) => {\n    setBitrixIds(prev => ({\n      ...prev,\n      [conversationId]: bitrixId\n    }));\n    console.log('ðŸ¢ ID Bitrix salvo:', conversationId, '->', bitrixId);\n  };\n\n  // FunÃ§Ã£o original para enviar mensagem (mantida)\n  const sendMessage = async (to, message) => {\n    if (!socketRef.current || !to || !message.trim()) {\n      throw new Error('Socket nÃ£o conectado ou dados invÃ¡lidos');\n    }\n\n    return new Promise((resolve, reject) => {\n      const timeout = setTimeout(() => {\n        reject(new Error('Timeout ao enviar mensagem'));\n      }, 30000);\n\n      const messageData = {\n        to: to.replace(/\\D/g, ''),\n        message: message.trim(),\n        timestamp: Date.now(),\n        bitrixId: bitrixIds[to] || null // Incluir ID do Bitrix se existir\n      };\n\n      console.log('ðŸ“¤ Enviando mensagem:', messageData);\n\n      const handleResponse = (response) => {\n        clearTimeout(timeout);\n        if (response?.success) {\n          resolve(response);\n        } else {\n          reject(new Error(response?.error || 'Erro ao enviar mensagem'));\n        }\n      };\n\n      socketRef.current.once('message:sent', handleResponse);\n      socketRef.current.once('message:error', (error) => {\n        clearTimeout(timeout);\n        reject(new Error(error.message || 'Erro ao enviar mensagem'));\n      });\n\n      socketRef.current.emit('sendMessage', messageData);\n    });\n  };\n\n  // FunÃ§Ã£o para conectar WhatsApp\n  const connectWhatsApp = () => {\n    if (socketRef.current) {\n      console.log('ðŸ”„ Conectando WhatsApp...');\n      socketRef.current.emit('whatsapp:connect');\n    }\n  };\n\n  // FunÃ§Ã£o para obter chats\n  const getChats = () => {\n    if (socketRef.current) {\n      console.log('ðŸ“‹ Solicitando lista de chats...');\n      socketRef.current.emit('whatsapp:getChats');\n    }\n  };\n\n  const messageSystemValue = {\n    socket: socketRef.current,\n    messages,\n    conversations,\n    connectionStatus,\n    sendMessage,\n    connectWhatsApp,\n    getChats,\n    setMessages,\n    setConversations,\n    // === NOVAS FUNCIONALIDADES ===\n    favoriteChats,\n    unreadChats,\n    chatNames,\n    bitrixIds,\n    toggleFavorite,\n    toggleUnread,\n    updateChatName,\n    setBitrixId\n  };\n\n  return (\n    <MessageContext.Provider value={messageSystemValue}>\n      {children}\n    </MessageContext.Provider>\n  );\n};\n\n// Context para mensagens\nconst MessageContext = React.createContext();\nexport const useMessages = () => {\n  const context = React.useContext(MessageContext);\n  if (!context) {\n    throw new Error('useMessages deve ser usado dentro de MessageSystem');\n  }\n  return context;\n};\n\n// ====================================\n// COMPONENTE DE CHAT INDIVIDUAL MELHORADO\n// ====================================\nconst ChatArea = ({ selectedChat, messages, onSendMessage, bitrixId, onBitrixIdChange }) => {\n  const [newMessage, setNewMessage] = useState('');\n  const [sending, setSending] = useState(false);\n  const [showEmojiPicker, setShowEmojiPicker] = useState(false);\n  const [isRecording, setIsRecording] = useState(false);\n  const [showAttachments, setShowAttachments] = useState(false); // Nova: mostrar anexos\n  const messagesEndRef = useRef(null);\n  const messageInputRef = useRef(null);\n\n  // Auto-scroll para Ãºltima mensagem\n  useEffect(() => {\n    messagesEndRef.current?.scrollIntoView({ behavior: 'smooth' });\n  }, [messages]);\n\n  const handleSendMessage = async (e) => {\n    e.preventDefault();\n    if (!newMessage.trim() || sending) return;\n\n    setSending(true);\n    try {\n      await onSendMessage(selectedChat.id, newMessage);\n      setNewMessage('');\n    } catch (error) {\n      console.error('Erro ao enviar mensagem:', error);\n      alert('Erro ao enviar mensagem: ' + error.message);\n    } finally {\n      setSending(false);\n    }\n  };\n\n  // FunÃ§Ã£o para inserir emoji\n  const handleEmojiSelect = (emoji) => {\n    const input = messageInputRef.current;\n    if (input) {\n      const cursorPos = input.selectionStart;\n      const textBefore = newMessage.substring(0, cursorPos);\n      const textAfter = newMessage.substring(cursorPos);\n      setNewMessage(textBefore + emoji + textAfter);\n      \n      // Reposicionar cursor\n      setTimeout(() => {\n        input.setSelectionRange(cursorPos + emoji.length, cursorPos + emoji.length);\n        input.focus();\n      }, 0);\n    }\n    setShowEmojiPicker(false);\n  };\n\n  // FunÃ§Ã£o para envio de Ã¡udio\n  const handleAudioRecorded = async (audioBlob) => {\n    try {\n      console.log('ðŸŽ™ï¸ Ãudio gravado:', audioBlob);\n      // Aqui vocÃª implementaria o envio do Ã¡udio\n      // Por enquanto, apenas simulamos\n      alert('ðŸŽ™ï¸ Ãudio gravado! (ImplementaÃ§Ã£o do envio em desenvolvimento)');\n      setIsRecording(false);\n    } catch (error) {\n      console.error('Erro ao enviar Ã¡udio:', error);\n      alert('Erro ao enviar Ã¡udio: ' + error.message);\n    }\n  };\n\n  const formatTime = (timestamp) => {\n    const date = new Date(timestamp);\n    return date.toLocaleTimeString('pt-BR', { \n      hour: '2-digit', \n      minute: '2-digit' \n    });\n  };\n\n  return (\n    <div style={styles.chatArea}>\n      {/* Header do Chat MELHORADO */}\n      <div style={styles.chatAreaHeader}>\n        <div style={styles.chatAvatar}>\n          {selectedChat.name?.charAt(0)?.toUpperCase() || '?'}\n        </div>\n        <div style={styles.chatInfo}>\n          <h3 style={styles.chatName}>{selectedChat.name}</h3>\n          <p style={styles.chatStatus}>Online</p>\n        </div>\n        \n        {/* === NOVA SEÃ‡ÃƒO: ID BITRIX + AÃ‡Ã•ES === */}\n        <div style={styles.chatActions}>\n          {/* Campo ID Bitrix */}\n          <div style={styles.bitrixContainer}>\n            <label style={styles.bitrixLabel}>ID Bitrix:</label>\n            <input\n              type=\"text\"\n              value={bitrixId || ''}\n              onChange={(e) => onBitrixIdChange(selectedChat.id, e.target.value)}\n              placeholder=\"Ex: 12345\"\n              style={styles.bitrixInput}\n            />\n          </div>\n          \n          {/* BotÃ£o Anexos */}\n          <button\n            onClick={() => setShowAttachments(!showAttachments)}\n            style={styles.actionButton}\n            title=\"Ver anexos da conversa\"\n          >\n            ðŸ“Ž\n          </button>\n        </div>\n      </div>\n\n      {/* === NOVA SEÃ‡ÃƒO: ÃREA DE ANEXOS (quando ativa) === */}\n      {showAttachments && (\n        <div style={styles.attachmentsArea}>\n          <h4 style={styles.attachmentsTitle}>ðŸ“Ž Arquivos desta conversa</h4>\n          <div style={styles.attachmentsList}>\n            <div style={styles.attachmentItem}>\n              <span>ðŸ“„ documento.pdf</span>\n              <button style={styles.attachmentAction}>ðŸ“¥</button>\n            </div>\n            <div style={styles.attachmentItem}>\n              <span>ðŸ–¼ï¸ imagem.jpg</span>\n              <button style={styles.attachmentAction}>ðŸ“¥</button>\n            </div>\n            <div style={styles.attachmentItem}>\n              <span>ðŸŽ¥ video.mp4</span>\n              <button style={styles.attachmentAction}>ðŸ“¥</button>\n            </div>\n            <p style={styles.attachmentsNote}>\n              Em desenvolvimento: Link para mensagem original\n            </p>\n          </div>\n        </div>\n      )}\n\n      {/* Lista de Mensagens (mantida igual) */}\n      <div style={styles.messagesContainer}>\n        {messages.length === 0 ? (\n          <div style={styles.emptyMessages}>\n            <p>ðŸ“± Nenhuma mensagem ainda</p>\n            <p>Envie uma mensagem para iniciar a conversa</p>\n          </div>\n        ) : (\n          messages.map((message, index) => (\n            <div\n              key={message.id || index}\n              style={{\n                ...styles.messageWrapper,\n                justifyContent: message.fromMe ? 'flex-end' : 'flex-start'\n              }}\n            >\n              <div\n                style={{\n                  ...styles.messageBubble,\n                  ...(message.fromMe ? styles.messageFromMe : styles.messageFromThem)\n                }}\n              >\n                <div style={styles.messageText}>\n                  {message.body || '[Mensagem vazia]'}\n                </div>\n                <div style={styles.messageTime}>\n                  {formatTime(message.timestamp)}\n                  {message.fromMe && <span style={styles.messageStatus}>âœ“</span>}\n                </div>\n              </div>\n            </div>\n          ))\n        )}\n        <div ref={messagesEndRef} />\n      </div>\n\n      {/* === INPUT DE MENSAGEM MELHORADO === */}\n      <div style={styles.messageForm}>\n        {/* Gravador de Ã¡udio */}\n        {isRecording ? (\n          <div style={styles.audioRecorder}>\n            <AudioRecorder\n              onRecordingComplete={handleAudioRecorded}\n              onCancel={() => setIsRecording(false)}\n              isVisible={true}\n            />\n          </div>\n        ) : (\n          <form onSubmit={handleSendMessage} style={styles.messageInputForm}>\n            {/* BotÃ£o Emoji */}\n            <button\n              type=\"button\"\n              onClick={() => setShowEmojiPicker(!showEmojiPicker)}\n              style={styles.emojiButton}\n              title=\"Inserir emoji\"\n            >\n              ðŸ˜€\n            </button>\n\n            {/* Input de mensagem */}\n            <input\n              ref={messageInputRef}\n              type=\"text\"\n              value={newMessage}\n              onChange={(e) => setNewMessage(e.target.value)}\n              placeholder=\"Digite sua mensagem...\"\n              style={styles.messageInput}\n              disabled={sending}\n            />\n\n            {/* BotÃ£o Anexar */}\n            <button\n              type=\"button\"\n              style={styles.attachButton}\n              title=\"Anexar arquivo\"\n              onClick={() => alert('ðŸ“Ž Anexar arquivo em desenvolvimento!')}\n            >\n              ðŸ“Ž\n            </button>\n\n            {/* BotÃ£o Ãudio */}\n            <button\n              type=\"button\"\n              onClick={() => setIsRecording(true)}\n              style={styles.audioButton}\n              title=\"Gravar Ã¡udio\"\n            >\n              ðŸŽ™ï¸\n            </button>\n\n            {/* BotÃ£o Enviar */}\n            <button \n              type=\"submit\" \n              style={{\n                ...styles.sendButton,\n                opacity: sending ? 0.5 : 1\n              }}\n              disabled={sending || !newMessage.trim()}\n            >\n              {sending ? 'â³' : 'ðŸ“¤'}\n            </button>\n          </form>\n        )}\n      </div>\n\n      {/* === SELETOR DE EMOJI === */}\n      <EmojiPicker\n        isVisible={showEmojiPicker}\n        onEmojiSelect={handleEmojiSelect}\n        onClose={() => setShowEmojiPicker(false)}\n      />\n    </div>\n  );\n};\n\n// ====================================\n// COMPONENTE DE ITEM DE CONVERSA MELHORADO\n// ====================================\nconst ConversationItem = ({ \n  conversation, \n  isSelected, \n  onClick, \n  isFavorite, \n  isUnread, \n  onToggleFavorite, \n  onToggleUnread, \n  onEditName \n}) => {\n  const [showMenu, setShowMenu] = useState(false);\n\n  return (\n    <div\n      style={{\n        ...styles.conversationItem,\n        ...(isSelected ? styles.conversationSelected : {}),\n        ...(isUnread ? styles.conversationUnread : {})\n      }}\n      onClick={onClick}\n      onContextMenu={(e) => {\n        e.preventDefault();\n        setShowMenu(!showMenu);\n      }}\n    >\n      {/* Avatar com indicador de favorito */}\n      <div style={styles.conversationAvatarContainer}>\n        <div style={styles.conversationAvatar}>\n          {conversation.name?.charAt(0)?.toUpperCase() || '?'}\n        </div>\n        {isFavorite && (\n          <div style={styles.favoriteIndicator}>â­</div>\n        )}\n      </div>\n      \n      <div style={styles.conversationInfo}>\n        <div style={styles.conversationName}>{conversation.name}</div>\n        <div style={styles.conversationLastMessage}>\n          {conversation.lastMessage || 'Sem mensagens'}\n        </div>\n      </div>\n      \n      {/* Badges */}\n      <div style={styles.conversationBadges}>\n        {conversation.unreadCount > 0 && (\n          <div style={styles.unreadBadge}>{conversation.unreadCount}</div>\n        )}\n        {isUnread && (\n          <div style={styles.manualUnreadBadge}>â—</div>\n        )}\n      </div>\n\n      {/* Menu de contexto */}\n      {showMenu && (\n        <div style={styles.contextMenu}>\n          <button \n            onClick={(e) => {\n              e.stopPropagation();\n              onToggleFavorite();\n              setShowMenu(false);\n            }}\n            style={styles.contextMenuItem}\n          >\n            {isFavorite ? 'ðŸ’” Remover favorito' : 'â­ Favoritar'}\n          </button>\n          <button \n            onClick={(e) => {\n              e.stopPropagation();\n              onToggleUnread();\n              setShowMenu(false);\n            }}\n            style={styles.contextMenuItem}\n          >\n            {isUnread ? 'âœ… Marcar como lida' : 'ðŸ“© Marcar como nÃ£o lida'}\n          </button>\n          <button \n            onClick={(e) => {\n              e.stopPropagation();\n              onEditName();\n              setShowMenu(false);\n            }}\n            style={styles.contextMenuItem}\n          >\n            âœï¸ Editar nome\n          </button>\n        </div>\n      )}\n    </div>\n  );\n};\n\n// ====================================\n// PÃGINA DE CHAT COMPLETA COM FUNCIONALIDADES AVANÃ‡ADAS\n// ====================================\nconst ChatPageComplete = () => {\n  const { \n    messages, \n    conversations, \n    sendMessage, \n    getChats, \n    connectionStatus,\n    favoriteChats,\n    unreadChats,\n    bitrixIds,\n    toggleFavorite,\n    toggleUnread,\n    updateChatName,\n    setBitrixId\n  } = useMessages();\n  \n  const [selectedConversation, setSelectedConversation] = useState(null);\n  const [searchTerm, setSearchTerm] = useState('');\n  const [showEditNameModal, setShowEditNameModal] = useState(false);\n  const [editingConversation, setEditingConversation] = useState(null);\n\n  // Carregar chats ao abrir pÃ¡gina\n  useEffect(() => {\n    if (connectionStatus === 'ready') {\n      getChats();\n    }\n  }, [connectionStatus, getChats]);\n\n  // Filtrar e ordenar conversas (favoritos primeiro)\n  const filteredConversations = conversations\n    .filter(conv => conv.name.toLowerCase().includes(searchTerm.toLowerCase()))\n    .sort((a, b) => {\n      const aFavorite = favoriteChats.has(a.id);\n      const bFavorite = favoriteChats.has(b.id);\n      \n      if (aFavorite && !bFavorite) return -1;\n      if (!aFavorite && bFavorite) return 1;\n      \n      return b.timestamp - a.timestamp; // Mais recente primeiro\n    });\n\n  // Abrir modal de editar nome\n  const handleEditName = (conversation) => {\n    setEditingConversation(conversation);\n    setShowEditNameModal(true);\n  };\n\n  // Salvar novo nome\n  const handleSaveName = (newName) => {\n    if (editingConversation && newName.trim()) {\n      updateChatName(editingConversation.id, newName.trim());\n    }\n    setShowEditNameModal(false);\n    setEditingConversation(null);\n  };\n\n  return (\n    <div style={styles.chatPageContainer}>\n      {/* SIDEBAR SEMPRE VISÃVEL Ã€ ESQUERDA (mantida + melhorada) */}\n      <div style={styles.chatSidebar}>\n        <div style={styles.sidebarHeader}>\n          <h3 style={styles.sidebarTitle}>ðŸ’¬ Conversas</h3>\n          <div style={styles.sidebarActions}>\n            <button \n              onClick={getChats}\n              style={styles.refreshButton}\n              title=\"Atualizar conversas\"\n            >\n              ðŸ”„\n            </button>\n          </div>\n        </div>\n        \n        {/* Busca */}\n        <div style={styles.searchContainer}>\n          <input\n            type=\"text\"\n            placeholder=\"Buscar conversas...\"\n            value={searchTerm}\n            onChange={(e) => setSearchTerm(e.target.value)}\n            style={styles.searchInput}\n          />\n        </div>\n\n        {/* Lista de Conversas MELHORADA */}\n        <div style={styles.conversationsList}>\n          {filteredConversations.length === 0 ? (\n            <div style={styles.emptyConversations}>\n              <p>Nenhuma conversa encontrada</p>\n              <button onClick={getChats} style={styles.loadButton}>\n                Carregar conversas\n              </button>\n            </div>\n          ) : (\n            filteredConversations.map(conv => (\n              <ConversationItem\n                key={conv.id}\n                conversation={conv}\n                isSelected={selectedConversation?.id === conv.id}\n                onClick={() => {\n                  console.log('ðŸ“± Selecionando conversa:', conv.name);\n                  setSelectedConversation(conv);\n                  // Marcar como lida ao abrir\n                  if (unreadChats.has(conv.id)) {\n                    toggleUnread(conv.id);\n                  }\n                }}\n                isFavorite={favoriteChats.has(conv.id)}\n                isUnread={unreadChats.has(conv.id)}\n                onToggleFavorite={() => toggleFavorite(conv.id)}\n                onToggleUnread={() => toggleUnread(conv.id)}\n                onEditName={() => handleEditName(conv)}\n              />\n            ))\n          )}\n        </div>\n      </div>\n\n      {/* ÃREA DE CHAT Ã€ DIREITA (melhorada) */}\n      <div style={styles.chatMainArea}>\n        {selectedConversation ? (\n          <ChatArea \n            selectedChat={selectedConversation}\n            messages={messages[selectedConversation.id] || []}\n            onSendMessage={sendMessage}\n            bitrixId={bitrixIds[selectedConversation.id] || ''}\n            onBitrixIdChange={setBitrixId}\n          />\n        ) : (\n          <div style={styles.noChatSelected}>\n            <div style={styles.welcomeMessage}>\n              <h2>ðŸ’¬ PRIMEM WhatsApp Business</h2>\n              <p>Selecione uma conversa da lista Ã  esquerda para comeÃ§ar</p>\n              <div style={styles.connectionStatus}>\n                <strong>Status:</strong> \n                <span style={{\n                  color: connectionStatus === 'ready' ? '#10B981' : '#EF4444',\n                  marginLeft: '8px'\n                }}>\n                  {connectionStatus === 'ready' ? 'ðŸŸ¢ Conectado' : 'ðŸ”´ Desconectado'}\n                </span>\n              </div>\n              {connectionStatus !== 'ready' && (\n                <p style={styles.helpText}>\n                  Verifique a conexÃ£o WhatsApp na pÃ¡gina de ConexÃ£o\n                </p>\n              )}\n              \n              {/* === NOVO: ESTATÃSTICAS === */}\n              <div style={styles.statsContainer}>\n                <div style={styles.statItem}>\n                  <strong>{conversations.length}</strong>\n                  <span>Conversas</span>\n                </div>\n                <div style={styles.statItem}>\n                  <strong>{favoriteChats.size}</strong>\n                  <span>Favoritas</span>\n                </div>\n                <div style={styles.statItem}>\n                  <strong>{unreadChats.size}</strong>\n                  <span>NÃ£o lidas</span>\n                </div>\n              </div>\n            </div>\n          </div>\n        )}\n      </div>\n\n      {/* === MODAL EDITAR NOME === */}\n      <EditNameModal\n        isOpen={showEditNameModal}\n        currentName={editingConversation?.name || ''}\n        onSave={handleSaveName}\n        onClose={() => {\n          setShowEditNameModal(false);\n          setEditingConversation(null);\n        }}\n        title=\"Editar Nome da Conversa\"\n        placeholder=\"Digite o novo nome...\"\n      />\n    </div>\n  );\n};\n\n// ====================================\n// COMPONENTE DE PROTEÃ‡ÃƒO (mantido igual)\n// ====================================\nconst ProtectedRoute = ({ children, requireAdmin = false }) => {\n  const { isLoggedIn, loading, user } = useAuth();\n\n  if (loading) {\n    return (\n      <div style={styles.loadingContainer}>\n        <div style={styles.loadingContent}>\n          <div style={styles.spinner}></div>\n          <h2 style={styles.loadingTitle}>PRIMEM WhatsApp</h2>\n          <p style={styles.loadingText}>Verificando permissÃµes...</p>\n        </div>\n      </div>\n    );\n  }\n\n  if (!isLoggedIn) {\n    return <Navigate to=\"/login\" replace />;\n  }\n\n  if (requireAdmin && user?.role !== 'admin') {\n    return (\n      <div style={styles.accessDeniedContainer}>\n        <div style={styles.accessDeniedContent}>\n          <h2 style={styles.accessDeniedTitle}>ðŸ”’ Acesso Restrito</h2>\n          <p style={styles.accessDeniedText}>\n            Apenas administradores podem acessar esta Ã¡rea.\n          </p>\n          <button onClick={() => window.history.back()} style={styles.backButton}>\n            â† Voltar\n          </button>\n        </div>\n      </div>\n    );\n  }\n\n  return children;\n};\n\n// ====================================\n// DASHBOARD PRINCIPAL (mantido igual)\n// ====================================\nconst MainDashboard = () => {\n  const navigate = useNavigate();\n  const { connectionStatus, connectWhatsApp, getChats } = useMessages();\n\n  const handleNavigateToChat = () => {\n    console.log('Navegando para Chat...');\n    navigate('/chat');\n  };\n\n  const handleNavigateToConnection = () => {\n    console.log('Navegando para ConexÃ£o...');\n    navigate('/connection');\n  };\n\n  const handleNavigateToSettings = () => {\n    console.log('Navegando para ConfiguraÃ§Ãµes...');\n    alert('âš™ï¸ ConfiguraÃ§Ãµes em desenvolvimento!');\n  };\n\n  useEffect(() => {\n    if (connectionStatus === 'disconnected') {\n      setTimeout(() => {\n        connectWhatsApp();\n        getChats();\n      }, 2000);\n    }\n  }, [connectionStatus, connectWhatsApp, getChats]);\n\n  return (\n    <DashboardPage\n      onNavigateToChat={handleNavigateToChat}\n      onNavigateToConnection={handleNavigateToConnection}\n      onNavigateToSettings={handleNavigateToSettings}\n      connectionStatus={connectionStatus}\n    />\n  );\n};\n\n// ====================================\n// ROTAS (mantidas iguais)\n// ====================================\nconst RootRoute = () => {\n  const { isLoggedIn, loading } = useAuth();\n  \n  if (loading) {\n    return (\n      <div style={styles.loadingContainer}>\n        <div style={styles.loadingContent}>\n          <div style={styles.spinner}></div>\n          <h2 style={styles.loadingTitle}>PRIMEM WhatsApp</h2>\n          <p style={styles.loadingText}>Iniciando sistema...</p>\n        </div>\n      </div>\n    );\n  }\n  \n  return isLoggedIn ? <Navigate to=\"/dashboard\" replace /> : <Navigate to=\"/login\" replace />;\n};\n\nconst LoginRoute = () => {\n  const { isLoggedIn, loading } = useAuth();\n  \n  if (loading) {\n    return (\n      <div style={styles.loadingContainer}>\n        <div style={styles.loadingContent}>\n          <div style={styles.spinner}></div>\n          <p style={styles.loadingText}>Verificando autenticaÃ§Ã£o...</p>\n        </div>\n      </div>\n    );\n  }\n  \n  return isLoggedIn ? <Navigate to=\"/dashboard\" replace /> : <LoginPage />;\n};\n\nconst AppRoutes = () => {\n  return (\n    <Routes>\n      <Route path=\"/\" element={<RootRoute />} />\n      <Route path=\"/login\" element={<LoginRoute />} />\n      <Route path=\"/dashboard\" element={\n        <ProtectedRoute>\n          <MainDashboard />\n        </ProtectedRoute>\n      } />\n      <Route path=\"/chat\" element={\n        <ProtectedRoute>\n          <ChatPageComplete />\n        </ProtectedRoute>\n      } />\n      <Route path=\"/connection\" element={\n        <ProtectedRoute>\n          <ConnectionPage />\n        </ProtectedRoute>\n      } />\n      <Route path=\"*\" element={<RootRoute />} />\n    </Routes>\n  );\n};\n\n// ====================================\n// COMPONENTE PRINCIPAL (mantido igual)\n// ====================================\nfunction App() {\n  return (\n    <BrowserRouter>\n      <SettingsProvider>\n        <AuthProvider>\n          <WhatsAppProvider>\n            <MessageSystem>\n              <div className=\"app\" style={{ minHeight: '100vh' }}>\n                <AppRoutes />\n                <style>{globalStyles}</style>\n              </div>\n            </MessageSystem>\n          </WhatsAppProvider>\n        </AuthProvider>\n      </SettingsProvider>\n    </BrowserRouter>\n  );\n}\n\n// ====================================\n// ESTILOS COMPLETOS + NOVOS ESTILOS\n// ====================================\nconst styles = {\n  // Estilos originais mantidos...\n  loadingContainer: {\n    display: 'flex',\n    justifyContent: 'center',\n    alignItems: 'center',\n    height: '100vh',\n    background: 'linear-gradient(135deg, #2B4C8C 0%, #8B9DC3 100%)',\n    color: 'white'\n  },\n  loadingContent: {\n    textAlign: 'center'\n  },\n  spinner: {\n    width: '50px',\n    height: '50px',\n    border: '4px solid rgba(255,255,255,0.3)',\n    borderTop: '4px solid white',\n    borderRadius: '50%',\n    animation: 'spin 2s linear infinite',\n    margin: '0 auto 24px'\n  },\n  loadingTitle: {\n    margin: '0 0 8px 0',\n    fontSize: '24px'\n  },\n  loadingText: {\n    margin: 0,\n    opacity: 0.8\n  },\n  accessDeniedContainer: {\n    display: 'flex',\n    flexDirection: 'column',\n    justifyContent: 'center',\n    alignItems: 'center',\n    height: '100vh',\n    background: '#f0f2f5',\n    padding: '20px'\n  },\n  accessDeniedContent: {\n    background: 'white',\n    borderRadius: '12px',\n    padding: '40px',\n    boxShadow: '0 4px 20px rgba(0,0,0,0.1)',\n    maxWidth: '500px',\n    textAlign: 'center'\n  },\n  accessDeniedTitle: {\n    color: '#EF4444',\n    marginBottom: '16px'\n  },\n  accessDeniedText: {\n    marginBottom: '20px'\n  },\n  backButton: {\n    padding: '12px 24px',\n    background: '#2B4C8C',\n    color: 'white',\n    border: 'none',\n    borderRadius: '8px',\n    cursor: 'pointer',\n    fontSize: '16px',\n    fontWeight: '600'\n  },\n  chatPageContainer: {\n    display: 'flex',\n    height: '100vh',\n    background: '#f0f2f5'\n  },\n  chatSidebar: {\n    width: '350px',\n    minWidth: '350px',\n    background: 'white',\n    borderRight: '1px solid #e5e7eb',\n    display: 'flex',\n    flexDirection: 'column',\n    boxShadow: '2px 0 8px rgba(0,0,0,0.1)'\n  },\n  sidebarHeader: {\n 