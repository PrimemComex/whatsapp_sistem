ce","overflow","textOverflow","wordWrap","borderBottomRightRadius","borderBottomLeftRadius","lineHeight","gap","outline","$RefreshReg$"],"sources":["C:/Users/david/Desktop/whatsapp_sistem/client/src/App.js"],"sourcesContent":["// client/src/App.js\n// =====================================\n// APP.JS COMPLETO v3.0 - LAYOUT CORRIGIDO\n// Sidebar sempre √† esquerda + Chat √† direita\n// =====================================\n\nimport React, { useEffect, useState, useRef } from 'react';\nimport { BrowserRouter, Routes, Route, Navigate, useNavigate } from 'react-router-dom';\nimport io from 'socket.io-client';\n\n// === CONTEXTOS - IMPORTS CORRETOS ===\nimport { AuthProvider, useAuth } from './contexts/AuthContext';\nimport { SettingsProvider } from './contexts/SettingsProvider';\nimport { WhatsAppProvider } from './contexts/WhatsAppContext';\n\n// === P√ÅGINAS - IMPORTS CORRETOS ===\nimport LoginPage from './pages/LoginPage';\nimport DashboardPage from './pages/DashboardPage';\nimport ConnectionPage from './pages/ConnectionPage';\n\n// === ESTILOS ===\nimport './App.css';\nimport './index.css';\n\n// === CONFIGURA√á√ïES ===\nconst API_URL = 'http://localhost:3001';\nconst SOCKET_URL = 'http://localhost:3001';\n\n// ====================================\n// SISTEMA DE MENSAGENS GLOBAL\n// ====================================\nconst MessageSystem = ({ children }) => {\n  const [socket, setSocket] = useState(null);\n  const [messages, setMessages] = useState({});\n  const [conversations, setConversations] = useState([]);\n  const [connectionStatus, setConnectionStatus] = useState('disconnected');\n  const socketRef = useRef(null);\n\n  // Inicializar Socket.IO\n  useEffect(() => {\n    console.log('üîå Inicializando Socket.IO...');\n    \n    const newSocket = io(SOCKET_URL, {\n      transports: ['polling'], // APENAS POLLING\n      reconnectionAttempts: 10,\n      reconnectionDelay: 1000,\n      timeout: 20000,\n    });\n\n    // Para debug - expor socket globalmente\n    if (typeof window !== 'undefined') {\n      window.socket = newSocket;\n    }\n\n    // Event Listeners\n    newSocket.on('connect', () => {\n      console.log('‚úÖ Socket conectado:', newSocket.id);\n      setConnectionStatus('connected');\n      setSocket(newSocket);\n      socketRef.current = newSocket;\n    });\n\n    newSocket.on('disconnect', () => {\n      console.log('‚ùå Socket desconectado');\n      setConnectionStatus('disconnected');\n    });\n\n    newSocket.on('reconnect', () => {\n      console.log('üîÑ Socket reconectado');\n      setConnectionStatus('connected');\n    });\n\n    // EVENTOS WHATSAPP\n    newSocket.on('whatsapp:message_received', (messageData) => {\n      console.log('üì® Mensagem recebida:', messageData);\n      \n      const conversationId = messageData.from;\n      setMessages(prev => ({\n        ...prev,\n        [conversationId]: [...(prev[conversationId] || []), {\n          id: messageData.id || Date.now(),\n          body: messageData.body || '',\n          from: messageData.from,\n          fromMe: false,\n          timestamp: messageData.timestamp || Date.now(),\n          type: 'received',\n          media: messageData.media,\n          hasMedia: messageData.hasMedia\n        }]\n      }));\n\n      // Atualizar lista de conversas\n      setConversations(prev => {\n        const existing = prev.find(conv => conv.id === conversationId);\n        if (existing) {\n          return prev.map(conv => \n            conv.id === conversationId \n              ? { ...conv, lastMessage: messageData.body, timestamp: messageData.timestamp }\n              : conv\n          );\n        } else {\n          return [...prev, {\n            id: conversationId,\n            name: messageData.notifyName || conversationId.split('@')[0],\n            lastMessage: messageData.body,\n            timestamp: messageData.timestamp,\n            unreadCount: 1\n          }];\n        }\n      });\n\n      // Notifica√ß√£o\n      if (Notification.permission === 'granted') {\n        new Notification('Nova mensagem WhatsApp', {\n          body: messageData.body,\n          icon: '/favicon.ico'\n        });\n      }\n    });\n\n    newSocket.on('whatsapp:message_sent', (messageData) => {\n      console.log('üì§ Mensagem enviada:', messageData);\n      \n      const conversationId = messageData.to;\n      setMessages(prev => ({\n        ...prev,\n        [conversationId]: [...(prev[conversationId] || []), {\n          id: messageData.id || Date.now(),\n          body: messageData.message || messageData.body || '',\n          to: messageData.to,\n          fromMe: true,\n          timestamp: messageData.timestamp || Date.now(),\n          type: 'sent',\n          status: 'sent'\n        }]\n      }));\n    });\n\n    newSocket.on('whatsapp:ready', (data) => {\n      console.log('üöÄ WhatsApp pronto:', data);\n      setConnectionStatus('ready');\n    });\n\n    newSocket.on('whatsapp:disconnected', () => {\n      console.log('‚ö†Ô∏è WhatsApp desconectado');\n      setConnectionStatus('disconnected');\n    });\n\n    newSocket.on('whatsapp:chats', (chats) => {\n      console.log('üí¨ Lista de chats recebida:', chats);\n      if (Array.isArray(chats)) {\n        setConversations(chats.map(chat => ({\n          id: chat.id._serialized,\n          name: chat.name,\n          lastMessage: chat.lastMessage?.body || '',\n          timestamp: chat.timestamp,\n          unreadCount: chat.unreadCount || 0,\n          isGroup: chat.isGroup\n        })));\n      }\n    });\n\n    // Cleanup\n    return () => {\n      console.log('üßπ Limpando Socket.IO...');\n      newSocket.disconnect();\n    };\n  }, []);\n\n  // Solicitar permiss√£o de notifica√ß√£o\n  useEffect(() => {\n    if (Notification.permission === 'default') {\n      Notification.requestPermission();\n    }\n  }, []);\n\n  // Fun√ß√£o global para enviar mensagem\n  const sendMessage = async (to, message) => {\n    if (!socketRef.current || !to || !message.trim()) {\n      throw new Error('Socket n√£o conectado ou dados inv√°lidos');\n    }\n\n    return new Promise((resolve, reject) => {\n      const timeout = setTimeout(() => {\n        reject(new Error('Timeout ao enviar mensagem'));\n      }, 30000);\n\n      const messageData = {\n        to: to.replace(/\\D/g, ''),\n        message: message.trim(),\n        timestamp: Date.now()\n      };\n\n      console.log('üì§ Enviando mensagem:', messageData);\n\n      const handleResponse = (response) => {\n        clearTimeout(timeout);\n        if (response?.success) {\n          resolve(response);\n        } else {\n          reject(new Error(response?.error || 'Erro ao enviar mensagem'));\n        }\n      };\n\n      socketRef.current.once('message:sent', handleResponse);\n      socketRef.current.once('message:error', (error) => {\n        clearTimeout(timeout);\n        reject(new Error(error.message || 'Erro ao enviar mensagem'));\n      });\n\n      socketRef.current.emit('sendMessage', messageData);\n    });\n  };\n\n  // Fun√ß√£o para conectar WhatsApp\n  const connectWhatsApp = () => {\n    if (socketRef.current) {\n      console.log('üîÑ Conectando WhatsApp...');\n      socketRef.current.emit('whatsapp:connect');\n    }\n  };\n\n  // Fun√ß√£o para obter chats\n  const getChats = () => {\n    if (socketRef.current) {\n      console.log('üìã Solicitando lista de chats...');\n      socketRef.current.emit('whatsapp:getChats');\n    }\n  };\n\n  const messageSystemValue = {\n    socket: socketRef.current,\n    messages,\n    conversations,\n    connectionStatus,\n    sendMessage,\n    connectWhatsApp,\n    getChats,\n    setMessages,\n    setConversations\n  };\n\n  return (\n    <MessageContext.Provider value={messageSystemValue}>\n      {children}\n    </MessageContext.Provider>\n  );\n};\n\n// Context para mensagens\nconst MessageContext = React.createContext();\nexport const useMessages = () => {\n  const context = React.useContext(MessageContext);\n  if (!context) {\n    throw new Error('useMessages deve ser usado dentro de MessageSystem');\n  }\n  return context;\n};\n\n// ====================================\n// COMPONENTE DE CHAT INDIVIDUAL\n// ====================================\nconst ChatArea = ({ selectedChat, messages, onSendMessage }) => {\n  const [newMessage, setNewMessage] = useState('');\n  const [sending, setSending] = useState(false);\n  const messagesEndRef = useRef(null);\n\n  // Auto-scroll para √∫ltima mensagem\n  useEffect(() => {\n    messagesEndRef.current?.scrollIntoView({ behavior: 'smooth' });\n  }, [messages]);\n\n  const handleSendMessage = async (e) => {\n    e.preventDefault();\n    if (!newMessage.trim() || sending) return;\n\n    setSending(true);\n    try {\n      await onSendMessage(selectedChat.id, newMessage);\n      setNewMessage('');\n    } catch (error) {\n      console.error('Erro ao enviar mensagem:', error);\n      alert('Erro ao enviar mensagem: ' + error.message);\n    } finally {\n      setSending(false);\n    }\n  };\n\n  const formatTime = (timestamp) => {\n    const date = new Date(timestamp);\n    return date.toLocaleTimeString('pt-BR', { \n      hour: '2-digit', \n      minute: '2-digit' \n    });\n  };\n\n  return (\n    <div style={styles.chatArea}>\n      {/* Header do Chat */}\n      <div style={styles.chatAreaHeader}>\n        <div style={styles.chatAvatar}>\n          {selectedChat.name?.charAt(0)?.toUpperCase() || '?'}\n        </div>\n        <div style={styles.chatInfo}>\n          <h3 style={styles.chatName}>{selectedChat.name}</h3>\n          <p style={styles.chatStatus}>Online</p>\n        </div>\n      </div>\n\n      {/* Lista de Mensagens */}\n      <div style={styles.messagesContainer}>\n        {messages.length === 0 ? (\n          <div style={styles.emptyMessages}>\n            <p>üì± Nenhuma mensagem ainda</p>\n            <p>Envie uma mensagem para iniciar a conversa</p>\n          </div>\n        ) : (\n          messages.map((message, index) => (\n            <div\n              key={message.id || index}\n              style={{\n                ...styles.messageWrapper,\n                justifyContent: message.fromMe ? 'flex-end' : 'flex-start'\n              }}\n            >\n              <div\n                style={{\n                  ...styles.messageBubble,\n                  ...(message.fromMe ? styles.messageFromMe : styles.messageFromThem)\n                }}\n              >\n                <div style={styles.messageText}>\n                  {message.body || '[Mensagem vazia]'}\n                </div>\n                <div style={styles.messageTime}>\n                  {formatTime(message.timestamp)}\n                  {message.fromMe && <span style={styles.messageStatus}>‚úì</span>}\n                </div>\n              </div>\n            </div>\n          ))\n        )}\n        <div ref={messagesEndRef} />\n      </div>\n\n      {/* Input de Mensagem */}\n      <form onSubmit={handleSendMessage} style={styles.messageForm}>\n        <input\n          type=\"text\"\n          value={newMessage}\n          onChange={(e) => setNewMessage(e.target.value)}\n          placeholder=\"Digite sua mensagem...\"\n          style={styles.messageInput}\n          disabled={sending}\n        />\n        <button \n          type=\"submit\" \n          style={{\n            ...styles.sendButton,\n            opacity: sending ? 0.5 : 1\n          }}\n          disabled={sending || !newMessage.trim()}\n        >\n          {sending ? '‚è≥' : 'üì§'}\n        </button>\n      </form>\n    </div>\n  );\n};\n\n// ====================================\n// P√ÅGINA DE CHAT COMPLETA COM LAYOUT FIXO\n// ====================================\nconst ChatPageComplete = () => {\n  const { messages, conversations, sendMessage, getChats, connectionStatus } = useMessages();\n  const [selectedConversation, setSelectedConversation] = useState(null);\n  const [searchTerm, setSearchTerm] = useState('');\n\n  // Carregar chats ao abrir p√°gina\n  useEffect(() => {\n    if (connectionStatus === 'ready') {\n      getChats();\n    }\n  }, [connectionStatus, getChats]);\n\n  // Filtrar conversas por busca\n  const filteredConversations = conversations.filter(conv =>\n    conv.name.toLowerCase().includes(searchTerm.toLowerCase())\n  );\n\n  return (\n    <div style={styles.chatPageContainer}>\n      {/* SIDEBAR SEMPRE VIS√çVEL √Ä ESQUERDA */}\n      <div style={styles.chatSidebar}>\n        <div style={styles.sidebarHeader}>\n          <h3 style={styles.sidebarTitle}>üí¨ Conversas</h3>\n          <button \n            onClick={getChats}\n            style={styles.refreshButton}\n            title=\"Atualizar conversas\"\n          >\n            üîÑ\n          </button>\n        </div>\n        \n        {/* Busca */}\n        <div style={styles.searchContainer}>\n          <input\n            type=\"text\"\n            placeholder=\"Buscar conversas...\"\n            value={searchTerm}\n            onChange={(e) => setSearchTerm(e.target.value)}\n            style={styles.searchInput}\n          />\n        </div>\n\n        {/* Lista de Conversas */}\n        <div style={styles.conversationsList}>\n          {filteredConversations.length === 0 ? (\n            <div style={styles.emptyConversations}>\n              <p>Nenhuma conversa encontrada</p>\n              <button onClick={getChats} style={styles.loadButton}>\n                Carregar conversas\n              </button>\n            </div>\n          ) : (\n            filteredConversations.map(conv => (\n              <div\n                key={conv.id}\n                style={{\n                  ...styles.conversationItem,\n                  ...(selectedConversation?.id === conv.id ? styles.conversationSelected : {})\n                }}\n                onClick={() => {\n                  console.log('üì± Selecionando conversa:', conv.name);\n                  setSelectedConversation(conv);\n                }}\n              >\n                <div style={styles.conversationAvatar}>\n                  {conv.name?.charAt(0)?.toUpperCase() || '?'}\n                </div>\n                <div style={styles.conversationInfo}>\n                  <div style={styles.conversationName}>{conv.name}</div>\n                  <div style={styles.conversationLastMessage}>\n                    {conv.lastMessage || 'Sem mensagens'}\n                  </div>\n                </div>\n                {conv.unreadCount > 0 && (\n                  <div style={styles.unreadBadge}>{conv.unreadCount}</div>\n                )}\n              </div>\n            ))\n          )}\n        </div>\n      </div>\n\n      {/* √ÅREA DE CHAT √Ä DIREITA */}\n      <div style={styles.chatMainArea}>\n        {selectedConversation ? (\n          <ChatArea \n            selectedChat={selectedConversation}\n            messages={messages[selectedConversation.id] || []}\n            onSendMessage={sendMessage}\n          />\n        ) : (\n          <div style={styles.noChatSelected}>\n            <div style={styles.welcomeMessage}>\n              <h2>üí¨ PRIMEM WhatsApp Business</h2>\n              <p>Selecione uma conversa da lista √† esquerda para come√ßar</p>\n              <div style={styles.connectionStatus}>\n                <strong>Status:</strong> \n                <span style={{\n                  color: connectionStatus === 'ready' ? '#10B981' : '#EF4444',\n                  marginLeft: '8px'\n                }}>\n                  {connectionStatus === 'ready' ? 'üü¢ Conectado' : 'üî¥ Desconectado'}\n                </span>\n              </div>\n              {connectionStatus !== 'ready' && (\n                <p style={styles.helpText}>\n                  Verifique a conex√£o WhatsApp na p√°gina de Conex√£o\n                </p>\n              )}\n            </div>\n          </div>\n        )}\n      </div>\n    </div>\n  );\n};\n\n// ====================================\n// COMPONENTE DE PROTE√á√ÉO MELHORADO\n// ====================================\nconst ProtectedRoute = ({ children, requireAdmin = false }) => {\n  const { isLoggedIn, loading, user } = useAuth();\n\n  if (loading) {\n    return (\n      <div style={styles.loadingContainer}>\n        <div style={styles.loadingContent}>\n          <div style={styles.spinner}></div>\n          <h2 style={styles.loadingTitle}>PRIMEM WhatsApp</h2>\n          <p style={styles.loadingText}>Verificando permiss√µes...</p>\n        </div>\n      </div>\n    );\n  }\n\n  if (!isLoggedIn) {\n    return <Navigate to=\"/login\" replace />;\n  }\n\n  if (requireAdmin && user?.role !== 'admin') {\n    return (\n      <div style={styles.accessDeniedContainer}>\n        <div style={styles.accessDeniedContent}>\n          <h2 style={styles.accessDeniedTitle}>üîí Acesso Restrito</h2>\n          <p style={styles.accessDeniedText}>\n            Apenas administradores podem acessar esta √°rea.\n          </p>\n          <button onClick={() => window.history.back()} style={styles.backButton}>\n            ‚Üê Voltar\n          </button>\n        </div>\n      </div>\n    );\n  }\n\n  return children;\n};\n\n// ====================================\n// DASHBOARD PRINCIPAL COM NAVEGA√á√ÉO\n// ====================================\nconst MainDashboard = () => {\n  const navigate = useNavigate();\n  const { connectionStatus, connectWhatsApp, getChats } = useMessages();\n\n  const handleNavigateToChat = () => {\n    console.log('Navegando para Chat...');\n    navigate('/chat');\n  };\n\n  const handleNavigateToConnection = () => {\n    console.log('Navegando para Conex√£o...');\n    navigate('/connection');\n  };\n\n  const handleNavigateToSettings = () => {\n    console.log('Navegando para Configura√ß√µes...');\n    alert('‚öôÔ∏è Configura√ß√µes em desenvolvimento!');\n  };\n\n  // Auto-conectar WhatsApp se n√£o estiver conectado\n  useEffect(() => {\n    if (connectionStatus === 'disconnected') {\n      setTimeout(() => {\n        connectWhatsApp();\n        getChats();\n      }, 2000);\n    }\n  }, [connectionStatus, connectWhatsApp, getChats]);\n\n  return (\n    <DashboardPage\n      onNavigateToChat={handleNavigateToChat}\n      onNavigateToConnection={handleNavigateToConnection}\n      onNavigateToSettings={handleNavigateToSettings}\n      connectionStatus={connectionStatus}\n    />\n  );\n};\n\n// ====================================\n// ROTAS\n// ====================================\nconst RootRoute = () => {\n  const { isLoggedIn, loading } = useAuth();\n  \n  if (loading) {\n    return (\n      <div style={styles.loadingContainer}>\n        <div style={styles.loadingContent}>\n          <div style={styles.spinner}></div>\n          <h2 style={styles.loadingTitle}>PRIMEM WhatsApp</h2>\n          <p style={styles.loadingText}>Iniciando sistema...</p>\n        </div>\n      </div>\n    );\n  }\n  \n  return isLoggedIn ? <Navigate to=\"/dashboard\" replace /> : <Navigate to=\"/login\" replace />;\n};\n\nconst LoginRoute = () => {\n  const { isLoggedIn, loading } = useAuth();\n  \n  if (loading) {\n    return (\n      <div style={styles.loadingContainer}>\n        <div style={styles.loadingContent}>\n          <div style={styles.spinner}></div>\n          <p style={styles.loadingText}>Verificando autentica√ß√£o...</p>\n        </div>\n      </div>\n    );\n  }\n  \n  return isLoggedIn ? <Navigate to=\"/dashboard\" replace /> : <LoginPage />;\n};\n\nconst AppRoutes = () => {\n  return (\n    <Routes>\n      <Route path=\"/\" element={<RootRoute />} />\n      <Route path=\"/login\" element={<LoginRoute />} />\n      <Route path=\"/dashboard\" element={\n        <ProtectedRoute>\n          <MainDashboard />\n        </ProtectedRoute>\n      } />\n      <Route path=\"/chat\" element={\n        <ProtectedRoute>\n          <ChatPageComplete />\n        </ProtectedRoute>\n      } />\n      <Route path=\"/connection\" element={\n        <ProtectedRoute>\n          <ConnectionPage />\n        </ProtectedRoute>\n      } />\n      <Route path=\"*\" element={<RootRoute />} />\n    </Routes>\n  );\n};\n\n// ====================================\n// COMPONENTE PRINCIPAL\n// ====================================\nfunction App() {\n  return (\n    <BrowserRouter>\n      <SettingsProvider>\n        <AuthProvider>\n          <WhatsAppProvider>\n            <MessageSystem>\n              <div className=\"app\" style={{ minHeight: '100vh' }}>\n                <AppRoutes />\n                <style>{globalStyles}</style>\n              </div>\n            </MessageSystem>\n          </WhatsAppProvider>\n        </AuthProvider>\n      </SettingsProvider>\n    </BrowserRouter>\n  );\n}\n\n// ====================================\n// ESTILOS COMPLETOS\n// ====================================\nconst styles = {\n  // Loading States\n  loadingContainer: {\n    display: 'flex',\n    justifyContent: 'center',\n    alignItems: 'center',\n    height: '100vh',\n    background: 'linear-gradient(135deg, #2B4C8C 0%, #8B9DC3 100%)',\n    color: 'white'\n  },\n  loadingContent: {\n    textAlign: 'center'\n  },\n  spinner: {\n    width: '50px',\n    height: '50px',\n    border: '4px solid rgba(255,255,255,0.3)',\n    borderTop: '4px solid white',\n    borderRadius: '50%',\n    animation: 'spin 2s linear infinite',\n    margin: '0 auto 24px'\n  },\n  loadingTitle: {\n    margin: '0 0 8px 0',\n    fontSize: '24px'\n  },\n  loadingText: {\n    margin: 0,\n    opacity: 0.8\n  },\n\n  // Access Denied\n  accessDeniedContainer: {\n    display: 'flex',\n    flexDirection: 'column',\n    justifyContent: 'center',\n    alignItems: 'center',\n    height: '100vh',\n    background: '#f0f2f5',\n    padding: '20px'\n  },\n  accessDeniedContent: {\n    background: 'white',\n    borderRadius: '12px',\n    padding: '40px',\n    boxShadow: '0 4px 20px rgba(0,0,0,0.1)',\n    maxWidth: '500px',\n    textAlign: 'center'\n  },\n  accessDeniedTitle: {\n    color: '#EF4444',\n    marginBottom: '16px'\n  },\n  accessDeniedText: {\n    marg